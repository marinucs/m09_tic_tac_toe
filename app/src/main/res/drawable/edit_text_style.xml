<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="@dimen/et_depth"
        android:top="@dimen/et_depth"
        android:start="@dimen/et_depth"
        android:end="@dimen/et_depth">
        <shape
            android:shape="rectangle">
            <stroke
                android:width="5dp"
                android:color="@color/color_background_dark"/>
            <corners
                android:radius="1dp" />
            <solid
                android:color="#1A000000" />
        </shape>
    </item>
</layer-list>